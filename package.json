{
  "name": "event-radar-web",
  "version": "1.0.0",
  "private": true,
  "description": "Event manager tool",
  "homepage": "",
  "license": "See LICENSE file",
  "author": {
    "name": "Chetan Patil"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "lint": "npm run prettier && npm run lint:js",
    "lint:commit": "commitlint --edit",
    "lint:commit:head": "commitlint --from=HEAD~1",
    "lint:fix": "npm run prettier:fix && npm run lint:js:fix",
    "lint:js": "eslint ./src",
    "lint:js:fix": "eslint ./src --fix",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "preview": "vite preview",
    "start": "vite",
    "tsc:start": "tsc --noEmit --watch"
  },
  "lint-staged": {
    "./**/*.{js,ts,jsx,tsx,mdx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "./**/*.{json,md,yml,yaml,sh,xml,svg,html}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/format": "^19.5.0",
    "@eslint/js": "^9.10.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.10",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.2",
    "prettier-plugin-sh": "^0.14.0",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.4.5",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  }
}
